    "@angular/compiler-cli": "18.1.1",
    "@angular/language-service": "18.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash-es": "^4.17.4",
    "@types/luxon": "3.0.0",
    "@types/node": "^12.19.15",
